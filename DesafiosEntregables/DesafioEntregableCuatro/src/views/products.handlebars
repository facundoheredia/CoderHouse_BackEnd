<div id="bodyProductos" class="container-fluid">
    <div class="tituloSeccion d-flex justify-content-center align-items-center">
        <h3 class="m-0">PRODUCTOS EN LISTA</h3>
    </div>
    <div class="p-4">
  <table class="table align-middle">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">TITLE</th>
        <th scope="col">DESCRIPTION</th>
        <th scope="col">PRICE</th>
        <th scope="col">CATEGORY</th>
        <th scope="col">CODE</th>
        <th scope="col">STOCK</th>
        <th scope="col">STATUS</th>
      </tr>
    </thead>
    <tbody class="table-group-divider">
      {{#each productos}}
        <tr>
          <th scope="row">{{this.id}}</th>
          <td>{{this.title}}</td>
          <td>{{this.description}}</td>
          <td>$ {{this.price}}</td>
          <td>{{this.category}}</td>
          <td>{{this.code}}</td>
          <td>{{this.stock}}</td>
          <td><div
              class="badge text-bg-success d-flex justify-content-center align-items-center"
            ><p class="text-uppercase">{{this.status}}</p></div></td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
</div>
